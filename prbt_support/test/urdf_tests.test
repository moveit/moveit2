<launch>

  <!-- Load universal robot description format (URDF) -->
  <param name="robot_description" command="$(find xacro)/xacro --inorder $(find moveit_resources_prbt_support)/urdf/prbt.xacro"/>

  <!-- run test -->
  <test pkg="moveit_resources_prbt_support" test-name="urdf_tests" type="urdf_tests" >
    <param name="arm_tip_link" value="prbt_flange" />
  </test>

</launch>
